[Unit]
Description=Keywords500 Dashboard Service
After=network.target

[Service]
User=kkaemo
WorkingDirectory=/home/kkaemo/projects/keywords500
ExecStart=/home/kkaemo/projects/keywords500/.venv/bin/uvicorn dashboard:app --host 0.0.0.0 --port 8000
Restart=always
RestartSec=10
StandardOutput=journal
StandardError=journal

[Install]
WantedBy=multi-user.target 